#include <iostream>
using namespace std;
int main()
{
	int s, N[100], i;
	cout <<"Искомое число: ";
	cin >>s;
	if(s!=0) {
		cout <<"Введите последовательность: \n";
		for(i=0; i<100; i++) {
			cin >> N[i];
			if(N[i]==0)
				break;
			else if(N[i]==s) {
				cout<<"Число найдено: "<<N[i] << endl;
				return 0;
			}
		}
	}
	cout<<"Число не найдено.";
	return 0;
}

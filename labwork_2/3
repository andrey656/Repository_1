#include <iostream>
using namespace std;
int main()
{
	int i,sum=0,n;
	cout<<"n=";
	cin>>n;
	int* A = new int[n];
	cout<<"Введите числа:\n";
	for(i=0; i<n; i++) {
		cin>>A[i];
	}
	for(i=0; i<n; i++) {
		if (A[i]==0)break;
		else if (A[i]>0) {
			sum+=A[i];
		}
	}
	cout<<"\nСумма положительных чисел= "<<sum ;
	delete[] A;
	return 0;
}
